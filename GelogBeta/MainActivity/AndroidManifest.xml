        </activity>
        <activity android:name="com.geolog.GestoreAR" >
        </activity>
        <activity android:name="haseman.project.where4.HoldMeUp" >
        </activity>
        <activity android:name="com.tutorial.menuList.Facebook" >
        </activity>
        <activity android:name="com.geolog.web.WebService" >
